{

    "manifest_version": 2,
    "name": "Cbet Extension",
    "version": "1.6.3",
  
    "description": "Transmet le bet sur bet-analytix",
  
    "icons": {
      "21": "icons/icon-21.svg"
    },
    "permissions": [
      "storage"
    ],
  
    "content_scripts": [
      {
        "matches": ["*://*.cbet5.gg/*","*://*.app.bet-analytix.com/*"],
        "js": ["cbets.js"]
      }
    ]
  
  }
